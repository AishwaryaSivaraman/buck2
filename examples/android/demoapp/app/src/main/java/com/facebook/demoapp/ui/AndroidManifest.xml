<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.facebook.demoapp.ui">

  <application>

    <!--
    Note you can add activities to your top-level AndroidManifest.xml but you can also include them
    in a manifest for android_library() targets which can scale better for big projects.
     -->
    <activity
        android:name=".MainActivity"
        android:exported="true"
        android:theme="@style/Theme.MyApplication">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>

  </application>
</manifest>
